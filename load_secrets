unlock_bw_if_locked() {
  if [[ -z $BW_SESSION ]] ; then
    >&2 echo 'bw locked - unlocking into a new session'
    export BW_SESSION="$(bw unlock --raw)"
  fi
}
load_github() {
  unlock_bw_if_locked
  # Note that this github_pat_id value will be different for you!
  local -r github_pat_id='37fecffb-e59b-44c4-912f-72b4b9028279'
  local github_token
  github_token="$(bw get notes $github_pat_id)"
  export GITHUB_TOKEN="$github_token"
  export MRSK_REGISTRY_PASSWORD=`bw get notes MRSK_REGISTRY_PASSWORD`
}
load_github "$@"
